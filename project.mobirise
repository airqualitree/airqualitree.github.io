          "_sourceTheme": "studym5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"news1\" group=\"News\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"6\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"6\">\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"1\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"1\" condition=\"fullScreen == false\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>    \n        <header>Card</header>\n        <input type=\"checkbox\" title=\"Date\" name=\"showDate\">  \n        <input type=\"checkbox\" title=\"Title\" name=\"showCardTitle\" checked> \n        <input type=\"checkbox\" title=\"Text\" name=\"showText\" checked>  \n        <input type=\"checkbox\" title=\"Link\" name=\"showLink\" checked>        \n        <input type=\"color\" title=\"Body\" name=\"cardBg\" value=\"#ffffff\">\n        <input type=\"color\" title=\"Head\" name=\"dateBg\" value=\"#181818\">\n        <input type=\"range\" inline title=\"Amount\" name=\"amountNews\" min=\"1\" max=\"10\" step=\"1\" value=\"3\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Image\" value=\"/addons/studym5/components/_images/background18.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#333333\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#edeff0\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.3\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <h2 class=\"mbr-section-title mbr-fonts-style\" data-app-selector=\".mbr-section-title\" data-app-placeholder=\"Type Text\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\"><b><br></b><b>Get Started</b></h2>\n\n        <div class=\"row g-3\">\n            <div class=\"col-12 col-lg-4\">\n                <div class=\"wrapper\" mbr-if=\"amountNews > 0\">\n                    <div class=\"news-item\">\n                        <p class=\"date mbr-fonts-style m-0\" data-app-selector=\".date\" data-app-placeholder=\"Type Text\" mbr-theme-style=\"display-7\" mbr-if=\"showDate\"><b>June 12, 2021</b></p>\n                        <div class=\"news-content\">\n                            <h3 class=\"mbr-fonts-style card-title mb-4\" data-app-selector=\".card-title\" mbr-theme-style=\"display-5\" mbr-if=\"showCardTitle\"><b>Background</b></h3>\n                            <p class=\"mbr-fonts-style mbr-text\" data-app-selector=\".mbr-text,.link\" data-app-placeholder=\"Type Text\" mbr-theme-style=\"display-7\" mbr-if=\"showText\">View a timeline depicting the history of environmental justice in Los Angeles and across the country, and learn more about the data used in this project.</p>\n                                <div mbr-text class=\"py-3 link\" mbr-if=\"showLink\" mbr-theme-style=\"display-7\">\n                                    <a class=\"text-primary\" href=\"page1.html#top\">Read more</a>\n                                  </div>\n                        </div>\n                      \n                    </div>\n                </div>\n            </div>\n\n            <div class=\"col-12 col-lg-4\">\n                <div class=\"wrapper\" mbr-if=\"amountNews > 1\">\n                    <div class=\"news-item\">\n                        <p class=\"date mbr-fonts-style m-0\" data-app-selector=\".date\" data-app-placeholder=\"Type Text\" mbr-theme-style=\"display-7\" mbr-if=\"showDate\"><b>June 11, 2021</b></p>\n                        <div class=\"news-content\">\n                            <h3 class=\"mbr-fonts-style card-title mb-4\" data-app-selector=\".card-title\" mbr-theme-style=\"display-5\" mbr-if=\"showCardTitle\"><b>Narrative</b></h3>\n                            <p class=\"mbr-fonts-style mbr-text\" data-app-selector=\".mbr-text,.link\" data-app-placeholder=\"Type Text\" mbr-theme-style=\"display-7\" mbr-if=\"showText\">Learn about the relationship between race, socioeconomic status, human health, and environment. Explore the future of environmental justice research.</p>\n                                <div mbr-text class=\"py-3 link\" mbr-if=\"showLink\" mbr-theme-style=\"display-7\">\n                                    <a class=\"text-primary\" href=\"page5.html#top\">Read more</a>\n                                  </div>\n                        </div>\n                    \n                    </div>                    \n                </div>\n            </div>\n\n            <div class=\"col-12 col-lg-4\">\n                <div class=\"wrapper\" mbr-if=\"amountNews > 2\">\n                    <div class=\"news-item\">\n                        <p class=\"date mbr-fonts-style m-0\" data-app-selector=\".date\" data-app-placeholder=\"Type Text\" mbr-theme-style=\"display-7\" mbr-if=\"showDate\"><b>June 10, 2021</b></p